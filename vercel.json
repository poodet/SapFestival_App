{
  "buildCommand": "cd frontend && npm run export",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && npm install && cd ../api && npm install",
  "functions": {
    "api/**/*.ts": {
      "maxDuration": 60
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}